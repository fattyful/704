      <iSignal Name="AlarmAlerted" 		Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1"  Port="10020"/>
      
      <!-- Zones -->
      <iSignal Name="One"				Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1"	 Port="10010"/>
      <iSignal Name="Two"				Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1"	 Port="10010"/>
      <iSignal Name="Three"				Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1"	 Port="10010"/>
      <iSignal Name="Four"				Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1"	 Port="10010"/>
      <iSignal Name="Five"				Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1"	 Port="10010"/>
      <iSignal Name="Six"				Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1"	 Port="10010"/>
      <iSignal Name="Seven"				Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1"	 Port="10010"/>
      
      <iSignal Name="Faned" 	 Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1"  Port="10010"/>
