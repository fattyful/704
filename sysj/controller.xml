      <iSignal Name="TRed"   Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1"  Port="10010"/>
      
      <!-- From viz -->
      <iSignal Name="Occupied" 				Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1"  Port="10010"/>
      <iSignal Name="NOccupied" 			Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1"  Port="10010"/>
      <iSignal Name="UnderLightLimit"		Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1"  Port="10010"/>
