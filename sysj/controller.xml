      <iSignal Name="RightTemp" 				Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1"  Port="10010"/>
      
            
      <oSignal Name="BrightL"			To="ECSPlantCD.BrightL"  	Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1"   Port="10001" />
      <oSignal Name="BrightH"			To="ECSPlantCD.BrightH"  	Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1"   Port="10001" />
      <oSignal Name="RightLight"	To="ECSPlantCD.RightLight" 		Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1"   Port="10001" />
      <oSignal Name="AlarmAlert"	To="ECSPlantCD.AlarmAlert"      Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1"   Port="10001" />
       
      <oSignal Name="Fan"	To="ECSPlantCD.Fan"  	Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1"   Port="10001" />
      <oSignal Name="Heater"	To="ECSPlantCD.Heater"     Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1"   Port="10001" />
      <oSignal Name="TR"		To="ECSPlantCD.TR" 		Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1"   Port="10001" />
      
      <!--TO GUI  -->
      <oSignal Name="OccupiedE" 	To="UI.OccupiedE"	 	Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1"  	  Port="20000"/>
